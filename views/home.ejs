<%- include('partials/header'); -%>

<h1>Home</h1>
<% posts.forEach((post) => { %>
    <div id="postPreview">
        <h2><%= post.title %></h2>
        <p id="previewContent"><%= post.content %></p><a href="/posts/<%= post.title.replace(/\s/g, "-") %>">Read More</a>
    </div>
<% }); %>

<%- include('partials/footer'); -%>